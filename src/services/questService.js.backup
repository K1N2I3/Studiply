// Ëé∑ÂèñÁâπÂÆö‰ªªÂä°Êï∞ÊçÆ
export const getQuestData = (subject, category, questId) => {
  try {
    if (!questTemplates[subject] || !questTemplates[subject][category]) {
      return null
    }
    
    const quest = questTemplates[subject][category][questId]
    return quest || null
  } catch (error) {
    console.error('Error getting quest data:', error)
    return null
  }
}

// Ëé∑ÂèñÁî®Êà∑‰ªªÂä°ËøõÂ∫¶
export const getUserQuestProgress = async (userId) => {
  try {
    const progressRef = doc(db, 'questProgress', userId)
    const progressSnap = await getDoc(progressRef)
    
    if (!progressSnap.exists()) {
      // ÂàõÂª∫ÂàùÂßãËøõÂ∫¶
      const initialProgress = {
        userId,
        level: 1,
        totalXP: 0,
        gold: 0,
        completedQuests: [],
        completedBossBattles: [],
        skillTrees: {},
        crossSubjectSkills: {},
        achievements: [],
        portfolio: [],
        createdAt: serverTimestamp(),
        updatedAt: serverTimestamp()
      }
      
      // ÂàùÂßãÂåñË∑®Â≠¶ÁßëÊäÄËÉΩ
      Object.keys(crossSubjectSkills).forEach(skillId => {
        initialProgress.crossSubjectSkills[skillId] = {
          level: 0,
          xp: 0,
          maxXp: 100
        }
      })
      
      await setDoc(progressRef, initialProgress)
      return initialProgress
    }
    
    return progressSnap.data()
  } catch (error) {
    console.error('Error getting user quest progress:', error)
    throw error
  }
}

// Êõ¥Êñ∞‰ªªÂä°ËøõÂ∫¶
export const updateQuestProgress = async (userId, questId, subject, category, questType, xpEarned, goldEarned, deliverables) => {
  try {
    const progressRef = doc(db, 'questProgress', userId)
    const progressSnap = await getDoc(progressRef)
    
    let currentProgress = progressSnap.exists() ? progressSnap.data() : await getUserQuestProgress(userId)
    
    // Êõ¥Êñ∞Âü∫Êú¨ÁªüËÆ°
    currentProgress.totalXP += xpEarned
    currentProgress.gold += goldEarned
    
    // Ê∑ªÂä†ÂÆåÊàêÁöÑ‰ªªÂä°
    const questKey = `${subject}_${category}_${questId}`
    if (questType === 'quest') {
      if (!currentProgress.completedQuests.includes(questKey)) {
        currentProgress.completedQuests.push(questKey)
      }
    } else if (questType === 'boss-battle') {
      if (!currentProgress.completedBossBattles.includes(questKey)) {
        currentProgress.completedBossBattles.push(questKey)
      }
    }
    
    // Êõ¥Êñ∞ÊäÄËÉΩÁªèÈ™å
    const questTemplate = questTemplates[subject]?.[category]?.[questId]
    if (questTemplate && questTemplate.skills) {
      questTemplate.skills.forEach(skillId => {
        if (currentProgress.crossSubjectSkills[skillId]) {
          currentProgress.crossSubjectSkills[skillId].xp += xpEarned
          
          // Ê£ÄÊü•ÊòØÂê¶ÂçáÁ∫ß
          if (currentProgress.crossSubjectSkills[skillId].xp >= currentProgress.crossSubjectSkills[skillId].maxXp) {
            currentProgress.crossSubjectSkills[skillId].level += 1
            currentProgress.crossSubjectSkills[skillId].xp = 0
            currentProgress.crossSubjectSkills[skillId].maxXp = currentProgress.crossSubjectSkills[skillId].level * 100
          }
        }
      })
    }
    
    // Ê∑ªÂä†‰ΩúÂìÅÈõÜÈ°πÁõÆ
    if (deliverables && deliverables.length > 0) {
      const portfolioItem = {
        id: questKey,
        title: questTemplate?.title || 'Completed Quest',
        subject,
        category,
        questType,
        xpEarned,
        goldEarned,
        deliverables,
        completedAt: serverTimestamp()
      }
      currentProgress.portfolio.push(portfolioItem)
    }
    
    // Êõ¥Êñ∞Á≠âÁ∫ß
    const newLevel = Math.floor(currentProgress.totalXP / 1000) + 1
    if (newLevel > currentProgress.level) {
      currentProgress.level = newLevel
    }
    
    // ‰øùÂ≠òÊõ¥Êñ∞
    currentProgress.updatedAt = serverTimestamp()
    await updateDoc(progressRef, currentProgress)
    
    return currentProgress
  } catch (error) {
    console.error('Error updating quest progress:', error)
    throw error
  }
}

// Ê£ÄÊü•‰ªªÂä°ÊòØÂê¶Ëß£ÈîÅ
export const isQuestUnlocked = (questId, subject, category, userProgress) => {
  const questTemplate = questTemplates[subject]?.[category]?.[questId]
  if (!questTemplate) return false
  
  // Ê£ÄÊü•ÂâçÁΩÆ‰ªªÂä°
  if (questTemplate.requirements && questTemplate.requirements.length > 0) {
    return questTemplate.requirements.every(reqId => 
      userProgress.completedQuests.includes(`${subject}_${category}_${reqId}`)
    )
  }
  
  return true
}

// Ê£ÄÊü•‰ªªÂä°ÊòØÂê¶ÂÆåÊàê
export const isQuestCompleted = (questId, subject, category, userProgress) => {
  const questKey = `${subject}_${category}_${questId}`
  return userProgress.completedQuests.includes(questKey)
}

// Ëé∑ÂèñÂèØÁî®ÁöÑ‰ªªÂä°
export const getAvailableQuests = (subject, category, userProgress) => {
  const subjectQuests = questTemplates[subject]?.[category] || {}
  
  return Object.entries(subjectQuests).map(([questId, quest]) => ({
    ...quest,
    id: questId,
    isUnlocked: isQuestUnlocked(questId, subject, category, userProgress),
    isCompleted: isQuestCompleted(questId, subject, category, userProgress)
  }))
}

// AIÂØºÂ∏àÂª∫ËÆÆÁ≥ªÁªü
export const getAIMentorSuggestions = (userProgress) => {
  const suggestions = []
  
  // Âü∫‰∫éÁî®Êà∑ËøõÂ∫¶ÁîüÊàêÂª∫ËÆÆ
  if (userProgress.totalXP < 500) {
    suggestions.push({
      id: 'beginner-quest',
      title: 'Start with Beginner Quests',
      description: 'Complete some beginner-level quests to build your foundation',
      priority: 'high',
      category: 'learning-path'
    })
  }
  
  if (userProgress.completedQuests.length === 0) {
    suggestions.push({
      id: 'first-quest',
      title: 'Complete Your First Quest',
      description: 'Choose any available quest to get started on your learning journey',
      priority: 'high',
      category: 'motivation'
    })
  }
  
  return suggestions
}

