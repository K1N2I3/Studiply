{
  "manifest_version": 3,
  "name": "Studiply Focus Mode - Simple Test",
  "version": "1.0.0",
  "description": "Simple test extension",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background-simple.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-simple.js"],
      "run_at": "document_end"
    }
  ]
}
