{
  "manifest_version": 3,
  "name": "Minimal Test",
  "version": "1.0.0",
  "description": "Minimal test extension",
  "permissions": ["storage"],
  "background": {
    "service_worker": "background-minimal.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-minimal.js"]
    }
  ]
}
